      <TextField fx:id="filterField" layoutX="71.0" layoutY="26.0" />
      <ImageView fx:id="imageview" fitHeight="87.0" fitWidth="97.0" layoutX="118.0" layoutY="202.0" pickOnBounds="true" preserveRatio="true" />
      <FontAwesomeIconView layoutX="199.0" layoutY="250.0" onMouseClicked="#supprimerloc" />
