      <TextField fx:id="txtEmail" layoutX="85.0" layoutY="56.0" />
      

      <Button fx:id="btnAjout" layoutX="56.0" layoutY="233.0" mnemonicParsing="false" onAction="#ajouterloc" text="Ajouter" />
